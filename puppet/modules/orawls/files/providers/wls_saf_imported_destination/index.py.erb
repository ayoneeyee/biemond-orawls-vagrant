connect('<%= weblogicUser %>','<%= weblogicPassword %>'  ,'<%= weblogicConnectUrl %>')

def quote(text):
    if text:
        return "\"" + str(text).replace("\"", "\"\"") + "\""
    else: 
        return ""

m = ls('/JMSSystemResources')

f = open("/tmp/wlstScript.out", "w")
print >>f, "name;jmsmodule"
for token in m.split("dr--"):
    token=token.strip().lstrip().rstrip()
    if token:
        print '_jmsmodule_'+token+'___'
        cd('/JMSSystemResources/'+token+'/JMSResource/'+token)

        n = ls('SAFImportedDestinations')
        for token2 in n.split("dr--"):
            token2=token2.strip().lstrip().rstrip()
            if not token2 == '':
                cd('/JMSSystemResources/'+token+'/JMSResource/'+token+'/SAFImportedDestinations/'+token2)
                safRemoteMbean = cmo.getSAFRemoteContext()
                if safRemoteMbean:
                   path = getPath(safRemoteMbean)
                   cd("/"+path) 
                   safRemoteContext = get('Name')
                   cd('/JMSSystemResources/'+token+'/JMSResource/'+token+'/SAFImportedDestinations/'+token2)
                else:
                   safRemoteContext = ''

                jndiPrefix = cmo.getJNDIPrefix()
                safErrorHandlingMbean = cmo.getSAFErrorHandling()
                if safErrorHandlingMbean:
                   path2 = getPath(safErrorHandlingMbean)
                   cd("/"+path2) 
                   safErrorHandling = get('Name')
                   cd('/JMSSystemResources/'+token+'/JMSResource/'+token+'/SAFImportedDestinations/'+token2)
                else:
                   safErrorHandling = ''

                timeToLiveDefault       = cmo.getTimeToLiveDefault()
                #useSAFTimeToLiveDefault = cmo.getUseSAFTimeToLiveDefault()

                print >>f, ";".join(map(quote, [token+":"+token2,token]))
 
f.close()

disconnect()
exit()
